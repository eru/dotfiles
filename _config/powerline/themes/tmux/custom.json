{
    "default_module": "powerline.segments.common",
    "segments": {
        "left": [
            {
                "function": "user",
                "name": "user"
            }
        ],
        "right": [
            {
                "function": "uptime",
                "name": "uptime",
                "priority": 50
            },
            {
                "function": "system_load",
                "priority": 50
            },
            {
                "function": "date",
                "args": {
                    "format": "%a %m/%d %H:%M",
                    "istime": true
                }
            },
            {
                "function": "hostname",
                "name": "hostname"
            }
        ]
    }
}
