# /dotfiles/_bashrc_plus
# .bashrc を管理するとOSの違いで, しんどいんでこれを読み込ませる.

# えいりあす
alias la='ls -lah'
alias ll='ls -ll'
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'
alias cd..='cd ..'
## Git 関連
alias ga='git add'
alias gb='git branch'
alias gba='git branch -a'
alias gci='git commit'
alias gco='git checkout'
alias gd='git diff'
alias gl='git pull'
alias gp='git push'
alias gst='git status'
alias gss='git status -s'
